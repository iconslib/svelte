{
	"name": "@iconslib/svelte",
	"version": "0.0.5",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "npm run prepare:all && svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare:all": "node ./bin/prepare.mjs -a -p",
		"prepare:heroicons": "node ./bin/prepare.mjs -p heroicons",
		"prepare:ionicons": "node ./bin/prepare.mjs -p ionicons",
		"prepare:feather": "node ./bin/prepare.mjs -p feather"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./heroicons/solid": {
			"types": "./dist/heroicons/24/solid/index.d.ts",
			"svelte": "./dist/heroicons/24/solid/index.js"
		},
		"./heroicons/outline": {
			"types": "./dist/heroicons/24/outline/index.d.ts",
			"svelte": "./dist/heroicons/24/outline/index.js"
		},
		"./heroicons/16/solid": {
			"types": "./dist/heroicons/16/solid/index.d.ts",
			"svelte": "./dist/heroicons/16/solid/index.js"
		},
		"./heroicons/20/solid": {
			"types": "./dist/heroicons/20/solid/index.d.ts",
			"svelte": "./dist/heroicons/20/solid/index.js"
		},
		"./heroicons/24/solid": {
			"types": "./dist/heroicons/24/solid/index.d.ts",
			"svelte": "./dist/heroicons/24/solid/index.js"
		},
		"./heroicons/24/outline": {
			"types": "./dist/heroicons/24/outline/index.d.ts",
			"svelte": "./dist/heroicons/24/outline/index.js"
		},
		"./ionicons": {
			"types": "./dist/ionicons/index.d.ts",
			"svelte": "./dist/ionicons/index.js"
		},
		"./feather": {
			"types": "./dist/feather/index.d.ts",
			"svelte": "./dist/feather/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"cli-progress": "^3.12.0",
		"commander": "^12.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0-next.4",
		"feather-icons": "^4.29.1",
		"heroicons": "^2.1.3",
		"ionicons": "^7.3.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"svgo": "^3.2.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"edge.js": "^6.0.2"
	},
	"bin": {
		"cli": "./bin/run.js"
	},
	"oclif": {
		"bin": "cli",
		"commands": "./dist/commands",
		"dirname": "cli",
		"topicSeparator": " "
	}
}
